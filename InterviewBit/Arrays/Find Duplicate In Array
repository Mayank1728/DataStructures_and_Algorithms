
public class Solution {
    // DO NOT MODIFY THE ARGUMENTS WITH "final" PREFIX. IT IS READ ONLY
    public int repeatedNumber(final int[] A) {
        HashSet<Integer> hs= new HashSet<Integer>();
        for(int i: A)
        {
            if(hs.contains(i))
                return i;
            else
                hs.add(i);
        }
        return -1;
    }
}
