class Solution {
    public int majorityElement(int[] nums) {
        HashMap < Integer, Integer > hm = new HashMap < Integer, Integer > ();
        for (int i: nums) {
            int f = hm.getOrDefault(i, 0);
            if (f + 1 > nums.length / 2)
                return i;
            hm.put(i, f + 1);
        }
        return 0;
    }
}
