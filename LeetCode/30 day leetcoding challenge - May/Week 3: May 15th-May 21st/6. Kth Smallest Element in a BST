class Solution {
 static int i = 0;
 static int ans = 0;
 public void inorder(TreeNode root, int k) {
  if (root == null)
   return;
  kthSmallest(root.left, k);
  i++;
  if (k == i) {
   ans = root.val;

  }
  kthSmallest(root.right, k);

 }
 public int kthSmallest(TreeNode root, int k) {
  inorder(root, k);
  return ans;
 }
}
