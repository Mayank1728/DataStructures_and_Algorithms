
class Solution {
    public int compareVersion(String version1, String version2) {
        if (version1 == null || version2 == null || version1.length() == 0 || version2.length() == 0)
            return 0;
        List < String > v1 = new ArrayList(Arrays.asList(version1.split("\\.")));
        List < String > v2 = new ArrayList(Arrays.asList(version2.split("\\.")));
        while (v1.size() < v2.size()) {
            v1.add("0");
        }
        while (v2.size() < v1.size()) {
            v2.add("0");
        }
        int i = 0;
        while (i < v1.size()) {
            int a = Integer.parseInt(v1.get(i));
            int b = Integer.parseInt(v2.get(i));
            if (a > b)
                return 1;
            else if (a < b)
                return -1;
            else i++;
        }
        return 0;
    }
}
