class Solution {
    public int maxFrequency(int[] nums, int k) {
        Arrays.sort(nums);
        int maxFreq = 0;
        for(int i=nums.length-1 ; i>=0 ;i--){
          int curr = k;
          int freq= 1;
          for(int j = i-1 ; j>=0 ; j--){
            if(nums[i] - nums[j] <= curr){
              freq++;
              curr -= nums[i] - nums[j];
            }
            else
              break;
          }
          maxFreq = Math.max(maxFreq, freq);
          if(maxFreq >= i+1)
            return maxFreq;
        }
      return maxFreq;
    }
}
