class Solution {
    public ListNode deleteDuplicates(ListNode head) {
      if(head==null || head.next==null)
        return head;
        ListNode prev = head, curr=head.next;
        while(curr!=null){
          if(prev.val==curr.val){
            prev.next=curr.next;
          }
          else{
            prev=prev.next;
          }
         curr=curr.next;
        }
      return head;
        
    }
}
