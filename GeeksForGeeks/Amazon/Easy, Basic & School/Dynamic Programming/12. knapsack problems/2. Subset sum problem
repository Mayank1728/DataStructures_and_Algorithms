//1. Recursion

static boolean isSubsetSum(int set[], int n, int sum) {
	if (sum == 0) {
		return true;
	}
	if (n == 0) 
  return false;
	if (sum >= set[n - 1]) {
		return isSubsetSum(set, n - 1, sum - set[n - 1]) || isSubsetSum(set, n - 1, sum);
	} 
 else 
  return isSubsetSum(set, n - 1, sum);
}
