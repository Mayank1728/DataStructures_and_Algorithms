https://practice.geeksforgeeks.org/problems/number-of-coins/0

import java.util.*;
import java.lang.*;
import java.io.*;
class GFG {
    static int findMinNumberOfCoins(int[] arr, int k, int n) {
        int dp[] = new int[k + 1];
        Arrays.fill(dp, Integer.MAX_VALUE);
        dp[0] = 0;
        for (int i = 0; i <= k; i++) {
            for (int j = 0; j < n; j++) {
                if (arr[j] <= i) {
                    int subres = dp[i - arr[j]];
                    if (subres != Integer.MAX_VALUE && subres + 1 < dp[i])
                        dp[i] = subres + 1;
                }
            }
        }
        if (dp[k] == Integer.MAX_VALUE)
            return -1;
        return dp[k];
    }
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int t = sc.nextInt();
        for (int i = 0; i < t; i++) {
            int k = sc.nextInt();
            int n = sc.nextInt();
            int arr[] = new int[n];
            for (int j = 0; j < n; j++) {
                arr[j] = sc.nextInt();
            }
            System.out.println(findMinNumberOfCoins(arr, k, n));
        }
    }
}
